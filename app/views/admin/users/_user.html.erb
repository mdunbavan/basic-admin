<tr id="user-<%= user.id %>">
  <td><%= link_to user.name, admin_user_path(user) %></td>
  <td><%= user.email %></td>
  <td>
    <%= link_to "Edit", edit_admin_user_path(user), class: "btn btn-primary" %>
    <%= link_to "Delete", admin_user_path(user), method: :delete, remote: true, class: "btn btn-danger", data: {confirm: "Are you sure you want to delete #{user.name}?"} unless current_user == user  %>
  </td>
</tr>