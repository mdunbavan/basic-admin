<div class="row">
  <div class="col-md-4">
    <div class="panel panel-info">
      <div class="panel-heading">Recently created users</div>
      <div class="panel-body">
        <div class="list-group">
          <% User.order("created_at DESC").limit(5).each do |u| %>
            <%= link_to u.name, admin_user_path(u), class: "list-group-item" %>
          <% end %>
        </div>
      </div>
      <div class="panel-footer"><%= link_to "See all #{pluralize(User.count, "user")}", admin_users_path %></div>
    </div>

  </div>
  <div class="col-md-4">.col-md-4</div>
</div>